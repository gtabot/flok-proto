{
    "lexicon": 1,
    "id": "app.flox.flok.acceptInvite",
    "defs": {
        "main": {
            "type": "procedure",
            "description": "Accept an invitation to join a Flok.",
            "input": {
                "encoding": "application/json",
                "schema": {
                    "type": "object",
                    "required": [
                        "inviteId"
                    ],
                    "properties": {
                        "inviteId": {
                            "type": "string",
                            "description": "The invite to accept"
                        }
                    }
                }
            },
            "output": {
                "encoding": "application/json",
                "schema": {
                    "type": "object",
                    "required": [
                        "sessionId"
                    ],
                    "properties": {
                        "sessionId": {
                            "type": "string",
                            "description": "Session ID for accessing the Flok"
                        }
                    }
                }
            }
        }
    }
}